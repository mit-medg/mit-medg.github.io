	Mesh = {Boston; Computer Communication Networks; Computer Security; Computer Systems; Hospital Information Systems; Hospitals, Pediatric; Humans; Interinstitutional Relations; Medical Informatics; Medical Records Systems, Computerized; Pilot Projects; Research; Software Design; United States},
	Number = {3},
